#include <iostream>
using namespace std;

int main(){
    int space_index = 26, dot_index = 6, big_height = 0, smol_height = 5;
    for(int a = 0; a < 5; a++){
    for(int j = 0; j < space_index; j++) cout << ' ';
    for(int i = 0; i < dot_index; i++) cout << '*';
    for(int j = 0; j < big_height; j++) cout << ' '; cout << '*';
    cout << endl;
    for(int j = 0; j < space_index; j++) cout << ' '; cout << '*';
    for(int j = 0; j < smol_height; j++) cout << ' '; cout << '*';
    cout << endl;
    for(int j = 0; j < space_index; j++) cout << ' '; cout << '*';
    for(int j = 0; j < smol_height; j++) cout << ' '; cout << '*';
    space_index -= 6; smol_height+= 6; big_height += 6 ;
    cout << endl;
    }
    for(int i = 0; i < 32; i++) cout << '*';
}
