 int main(){
    //Task 1
    string fname= "KKKKKK", lname = "Ibrahim"; int count = 0, index = 0;
    bool isSubtr = 0, result = 0;
    for(int i = 0; i <  lname.length(); i++){
        while(index < fname.length()){
        isSubtr = fname[index] == lname[i] ? 1 : 0; 
        index++; count += isSubtr;
        }
        index = 0;
    }
    cout << (count ? "YES": "NO") << endl;
    return 0;
 }
