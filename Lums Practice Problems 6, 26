// Task 2
int prime(int n){
    int count = 0;
    for(int i = 1; i < n; i++){
        if(n % i == 0) count++;
    }
    if(count > 1) return 1;
    else return 0;
    return n;
}
int biggest(int n1, int n2){
    if(n1 > n2) return n1;
    else if(n1 == n2) return 0;
    else return n2;
}
int Fibonacci(int n){
    if(n == 1 || n == 2) return 1;
    else return Fibonacci(n - 1) + Fibonacci(n - 2);
}

